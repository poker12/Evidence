<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ChoiceBox?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.Separator?>
<?import javafx.scene.control.SplitPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.control.ToolBar?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<ScrollPane xmlns="http://javafx.com/javafx/8.0.60" xmlns:fx="http://javafx.com/fxml/1">
   <content>
      <VBox fx:id="paneProduct" fillWidth="true" snapToPixel="false">
         <children>
            <ToolBar prefHeight="4.0" prefWidth="200.0">
               <items>
                  <Button mnemonicParsing="false" text="Button" />
               </items>
            </ToolBar>
            <Separator />
            <SplitPane dividerPositions="0.5" prefHeight="0.0" prefWidth="1177.0">
               <items>
                  <VBox prefHeight="200.0" prefWidth="100.0" />
                  <HBox alignment="CENTER_RIGHT" prefHeight="0.0">
                     <children>
                        <ChoiceBox prefWidth="150.0" />
                        <TextField prefHeight="25.0" prefWidth="355.0" promptText="Vyhledávání" />
                     </children>
                  </HBox>
               </items>
            </SplitPane>
            <Separator />
            <TableView>
               <columns>
                  <TableColumn prefWidth="71.0" text="produkt" />
                  <TableColumn minWidth="0.0" prefWidth="96.0" text="kód produktu" />
                  <TableColumn prefWidth="110.0" text="cena za kus s DPH" />
                  <TableColumn prefWidth="107.0" text="cena za kus bez DPH" />
                  <TableColumn prefWidth="66.0" text="počet kusů" />
                  <TableColumn prefWidth="93.0" text="sazba DPH" />
                  <TableColumn prefWidth="145.0" text="naskladněno" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </content>
</ScrollPane>
